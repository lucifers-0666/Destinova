{
  "name": "destinova-backend",
  "version": "1.0.0",
  "description": "Destinova - Flight Booking Platform Backend API",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch src/server.ts",
    "dev:debug": "cross-env NODE_ENV=development tsx watch --inspect src/server.ts",
    "build": "tsc -p . && npm run copy-static",
    "copy-static": "node scripts/copy-static.js",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "start:prod": "npm run build && npm run start",
    "start:pm2": "pm2 start ecosystem.config.cjs --env production",
    "stop:pm2": "pm2 stop destinova",
    "restart:pm2": "pm2 restart destinova",
    "logs:pm2": "pm2 logs destinova",
    "seed": "cross-env NODE_ENV=development tsx src/seeders/seed.ts",
    "seed:flights": "cross-env NODE_ENV=development tsx src/seeders/seedFlights.ts",
    "seed:destinations": "cross-env NODE_ENV=development tsx src/seeders/seedDestinations.ts",
    "seed:users": "cross-env NODE_ENV=development tsx src/seeders/seedUsers.ts",
    "seed:all": "npm run seed",
    "seed:prod": "cross-env NODE_ENV=production node dist/seeders/seed.js",
    "train:pricing": "cross-env NODE_ENV=development tsx src/ai/trainModel.ts",
    "train:pricing:samples": "cross-env NODE_ENV=development tsx src/ai/trainModel.ts --samples=2000 --epochs=150",
    "generate:training-data": "cross-env NODE_ENV=development tsx src/ai/generateTrainingData.ts 1000",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.js",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:unit": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern=unit",
    "test:integration": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern=integration",
    "test:vitest": "vitest run",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "health": "curl -s http://localhost:4000/health | jq",
    "backup:db": "bash scripts/backup-db.sh",
    "restore:db": "bash scripts/restore-db.sh"
  },
  "keywords": [
    "flight-booking",
    "travel",
    "nodejs",
    "express",
    "typescript",
    "mongodb"
  ],
  "author": "Destinova Team",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/rate-limit": "^10.3.0",
    "@sentry/node": "^10.27.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.17.4",
    "@types/qrcode": "^1.5.6",
    "@types/stripe": "^8.0.416",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "bcryptjs": "^3.0.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.3.1",
    "fastify": "^4.29.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^9.0.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.10.1",
    "pdfkit": "^0.17.2",
    "pino": "^9.14.0",
    "pino-pretty": "^11.3.0",
    "qrcode": "^1.5.4",
    "redis": "^5.10.0",
    "sharp": "^0.34.5",
    "stripe": "^17.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3",
    "winston": "^3.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/hpp": "^0.2.7",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.25",
    "@types/nodemailer": "^6.4.21",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^10.3.0",
    "rimraf": "^6.0.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^1.6.1"
  }
}
